{
	"name": "vscode-ssh",
	"displayName": "vscode-ssh",
	"description": "ssh client for vscode",
	"version": "1.3.0",
	"publisher": "cweijan",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"ssh",
		"SSH",
		"Ssh",
		"tools",
		"manager"
	],
	"contributors": [
		{
			"name": "cweijan",
			"email": "cweijan@163.com"
		}
	],
	"badges": [],
	"icon": "resources/image/logo.png",
	"homepage": "https://github.com/cweijan/vscode-ssh/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/cweijan/vscode-ssh/issues",
		"email": "cweijan@163.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cweijan/vscode-ssh"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "github-cweijan-ssh",
					"title": "SSH",
					"icon": "resources/image/server-container.svg"
				}
			]
		},
		"views": {
			"github-cweijan-ssh": [
				{
					"id": "github.cweijan.ssh",
					"name": "SSH"
				}
			]
		},
		"commands": [
			{
				"command": "ssh.add",
				"title": "Add Connection",
				"category": "SSH",
				"icon": {
					"light": "resources/image/light/add.svg",
					"dark": "resources/image/dark/add.svg"
				}
			},
			{
				"command": "ssh.refresh",
				"title": "Refresh",
				"category": "SSH",
				"icon": {
					"light": "resources/image/light/refresh.svg",
					"dark": "resources/image/dark/refresh.svg"
				}
			},
			{
				"command": "ssh.connection.delete",
				"title": "Delete connection",
				"icon": "resources/image/delete.png"
			},
			{
				"command": "ssh.connection.terminal",
				"title": "Open Terminal",
				"icon": "resources/image/terminal.svg"
			},
			{
				"command": "ssh.terminal.hear",
				"title": "Open Terminal in hear"
			},
			{
				"command": "ssh.file.new",
				"title": "New File"
			},
			{
				"command": "ssh.folder.new",
				"title": "New Folder"
			},
			{
				"command": "ssh.IP.copy",
				"title": "Copy IP"
			},
			{
				"command": "ssh.forward.port",
				"title": "Forward Port"
			},
			{
				"command": "ssh.folder.open",
				"title": "Open In Terminal"
			},
			{
				"command": "ssh.file.delete",
				"title": "Delete"
			},
			{
				"command": "ssh.file.download",
				"title": "Download File"
			},
			{
				"command": "ssh.file.upload",
				"title": "Upload File"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "ssh.add",
					"when": "view == github.cweijan.ssh",
					"group": "navigation@3"
				},
				{
					"command": "ssh.refresh",
					"when": "view == github.cweijan.ssh",
					"group": "navigation@2"
				}
			],
			"view/item/context": [
				{
					"command": "ssh.connection.delete",
					"when": "view == github.cweijan.ssh && viewItem == connection",
					"group": "inline@0"
				},
				{
					"command": "ssh.connection.terminal",
					"when": "view == github.cweijan.ssh && viewItem == connection",
					"group": "inline@1"
				},
				{
					"command": "ssh.IP.copy",
					"when": "view == github.cweijan.ssh && viewItem == connection",
					"group": "connection@-2"
				},
				{
					"command": "ssh.forward.port",
					"when": "view == github.cweijan.ssh && viewItem == connection",
					"group": "connection@-1"
				},
				{
					"command": "ssh.file.new",
					"when": "view == github.cweijan.ssh && viewItem == connection",
					"group": "connection@0"
				},
				{
					"command": "ssh.folder.new",
					"when": "view == github.cweijan.ssh && viewItem == connection ",
					"group": "connection@1"
				},
				{
					"command": "ssh.file.new",
					"when": "view == github.cweijan.ssh && viewItem == folder",
					"group": "folder@0"
				},
				{
					"command": "ssh.folder.new",
					"when": "view == github.cweijan.ssh && viewItem == folder ",
					"group": "folder@1"
				},
				{
					"command": "ssh.file.upload",
					"when": "view == github.cweijan.ssh && viewItem == folder",
					"group": "folder@2"
				},
				{
					"command": "ssh.folder.open",
					"when": "view == github.cweijan.ssh && viewItem == folder",
					"group": "folder@3"
				},
				{
					"command": "ssh.file.delete",
					"when": "view == github.cweijan.ssh && viewItem == folder",
					"group": "folder@4"
				},
				{
					"command": "ssh.file.download",
					"when": "view == github.cweijan.ssh && viewItem == file",
					"group": "file@1"
				},
				{
					"command": "ssh.file.delete",
					"when": "view == github.cweijan.ssh && viewItem == file",
					"group": "file@2"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run build",
		"dev": "webpack --progress",
		"build": "webpack -p --progress"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.27",
		"@fortawesome/free-solid-svg-icons": "^5.12.1",
		"@types/node": "^13.11.0",
		"@types/ssh2": "^0.5.43",
		"@types/vscode": "1.38.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.4.2",
		"file-loader": "^5.1.0",
		"html-webpack-plugin": "^4.3.0",
		"style-loader": "^1.1.3",
		"ts-loader": "^7.0.5",
		"typescript": "^3.8.3",
		"url-loader": "^3.0.0",
		"vue-loader": "^15.9.2",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.6.11",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.11",
		"xterm": "^4.6.0",
		"xterm-addon-fit": "^0.3.0",
		"xterm-addon-search": "^0.5.0",
		"xterm-addon-web-links": "^0.2.1"
	},
	"dependencies": {
		"element-ui": "^2.13.2",
		"pretty-bytes": "^5.3.0",
		"ssh2": "^0.7.0",
		"tunnel-ssh": "^4.1.4",
		"vue": "^2.6.11",
		"debug": "2.6.9",
		"lodash.defaults": "^4.1.0"
	}
}
