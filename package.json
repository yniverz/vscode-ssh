{
  "name": "vscode-ssh",
  "displayName": "SSH Client",
  "description": "SSH Client for vscode",
  "version": "1.9.0",
  "publisher": "yniverz",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ssh",
    "SSH",
    "Ssh",
    "tools",
    "manager"
  ],
  "contributors": [
    {
      "name": "cweijan",
      "email": "cweijan@163.com"
    },
    {
      "name": "yniverz",
      "email": "yniverz01@gmail.com"
    }
  ],
  "badges": [],
  "icon": "resources/image/logo.png",
  "homepage": "https://github.com/yniverz/vscode-ssh/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/yniverz/vscode-ssh/issues",
    "email": "yniverz01@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yniverz/vscode-ssh"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "github-cweijan-ssh",
          "title": "SSH",
          "icon": "resources/image/server-container.svg"
        }
      ]
    },
    "views": {
      "github-cweijan-ssh": [
        {
          "id": "github.cweijan.ssh",
          "name": "SSH"
        }
      ]
    },
    "commands": [
      {
        "command": "ssh.add",
        "title": "Add Connection",
        "category": "SSH",
        "icon": {
          "light": "resources/image/light/add.svg",
          "dark": "resources/image/dark/add.svg"
        }
      },
      {
        "command": "ssh.edit",
        "title": "Edit Connection",
        "category": "SSH"
      },
      {
        "command": "ssh.refresh",
        "title": "Refresh",
        "category": "SSH",
        "icon": {
          "light": "resources/image/light/refresh.svg",
          "dark": "resources/image/dark/refresh.svg"
        }
      },
      {
        "command": "ssh.recover.connection",
        "title": "Recover Connection",
        "category": "SSH",
        "icon": {
          "light": "resources/image/light/refresh.svg",
          "dark": "resources/image/dark/refresh.svg"
        }
      },
      {
        "command": "ssh.connection.delete",
        "title": "Delete connection",
        "icon": {
          "light": "resources/image/light/delete.svg",
          "dark": "resources/image/dark/delete.png"
        }
      },
      {
        "command": "ssh.connection.terminal",
        "title": "Open Terminal",
        "icon": {
          "light": "resources/image/light/terminal.png",
          "dark": "resources/image/dark/terminal.svg"
        }
      },
      {
        "command": "ssh.terminal.hear",
        "title": "Open Terminal in hear"
      },
      {
        "command": "ssh.path.copy",
        "title": "Copy Path"
      },
      {
        "command": "ssh.file.new",
        "title": "New File"
      },
      {
        "command": "ssh.folder.new",
        "title": "New Folder"
      },
      {
        "command": "ssh.host.copy",
        "title": "Copy Host"
      },
      {
        "command": "ssh.forward.port",
        "title": "Forward Port"
      },
      {
        "command": "ssh.socks.port",
        "title": "Start Socks5 Proxy"
      },
      {
        "command": "ssh.folder.open",
        "title": "Open In Terminal"
      },
      {
        "command": "ssh.folder.syncLocal",
        "title": "Load Contents Locally"
      },
      {
        "command": "ssh.file.delete",
        "title": "Delete"
      },
      {
        "command": "ssh.file.download",
        "title": "Download File"
      },
      {
        "command": "ssh.file.upload",
        "title": "Upload File"
      },
      {
        "command": "ssh.filezilla",
        "title": "Open in FileZilla",
        "icon": {
          "light": "resources/image/light/link.svg",
          "dark": "resources/image/dark/link.svg"
        }
      },
      {
        "command": "ssh.export.connections",
        "title": "Export Connections",
        "category": "SSH",
        "icon": {
          "light": "resources/image/icon/zip.svg",
          "dark": "resources/image/icon/zip.svg"
        }
      },
      {
        "command": "ssh.import.connections",
        "title": "Import Connections",
        "category": "SSH",
        "icon": {
          "light": "resources/image/icon/xml.svg",
          "dark": "resources/image/icon/xml.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "ssh.add",
          "when": "view == github.cweijan.ssh",
          "group": "navigation@5"
        },
        {
          "command": "ssh.export.connections",
          "when": "view == github.cweijan.ssh",
          "group": "navigation@4"
        },
        {
          "command": "ssh.import.connections",
          "when": "view == github.cweijan.ssh",
          "group": "navigation@3"
        },
        {
          "command": "ssh.refresh",
          "when": "view == github.cweijan.ssh",
          "group": "navigation@2"
        },
        {
          "command": "ssh.recover.connection",
          "when": "view == github.cweijan.ssh",
          "group": "navigation@1"
        }
      ],
      "view/item/context": [
        {
          "command": "ssh.connection.terminal",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "inline@0"
        },
        {
          "command": "ssh.filezilla",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "inline@1"
        },
        {
          "command": "ssh.connection.delete",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "0_connection@-3"
        },
        {
          "command": "ssh.host.copy",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "0_connection@-2"
        },
        {
          "command": "ssh.edit",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "0_connection@-1"
        },
        {
          "command": "ssh.forward.port",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "1_connection@0"
        },
        {
          "command": "ssh.socks.port",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "1_connection@1"
        },
        {
          "command": "ssh.recover.connection",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "1_connection@2"
        },
        {
          "command": "ssh.file.new",
          "when": "view == github.cweijan.ssh && viewItem == connection",
          "group": "2_connection@2"
        },
        {
          "command": "ssh.folder.new",
          "when": "view == github.cweijan.ssh && viewItem == connection ",
          "group": "2_connection@4"
        },
        {
          "command": "ssh.path.copy",
          "when": "view == github.cweijan.ssh && viewItem != connection",
          "group": "a@1"
        },
        {
          "command": "ssh.folder.open",
          "when": "view == github.cweijan.ssh && viewItem == folder",
          "group": "a@2"
        },
        {
          "command": "ssh.file.new",
          "when": "view == github.cweijan.ssh && viewItem == folder",
          "group": "folder@0"
        },
        {
          "command": "ssh.folder.new",
          "when": "view == github.cweijan.ssh && viewItem == folder ",
          "group": "folder@1"
        },
        {
          "command": "ssh.file.upload",
          "when": "view == github.cweijan.ssh && viewItem == folder",
          "group": "folder@2"
        },
        {
          "command": "ssh.folder.syncLocal",
          "when": "view == github.cweijan.ssh && viewItem == folder",
          "group": "folder@3"
        },
        {
          "command": "ssh.recover.connection",
          "when": "view == github.cweijan.ssh && viewItem == folder",
          "group": "folder@4"
        },
        {
          "command": "ssh.file.delete",
          "when": "view == github.cweijan.ssh && viewItem == folder",
          "group": "folder@4"
        },
        {
          "command": "ssh.file.download",
          "when": "view == github.cweijan.ssh && viewItem == file",
          "group": "file@1"
        },
        {
          "command": "ssh.recover.connection",
          "when": "view == github.cweijan.ssh && viewItem == file",
          "group": "file@3"
        },
        {
          "command": "ssh.file.delete",
          "when": "view == github.cweijan.ssh && viewItem == file",
          "group": "file@2"
        },
        {
          "command": "ssh.recover.connection",
          "when": "view == github.cweijan.ssh && viewItem == info",
          "group": "info@1"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "SSH Client",
      "properties": {
        "vscode-ssh.terimanlTitle": {
          "type": "string",
          "enum": [
            "user@host",
            "connectionName"
          ],
          "default": "connectionName",
          "description": "Set ssh terminal title."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "dev": "webpack --progress",
    "build": "webpack -p --progress",
    "postpack": "cd .. && sudo cp -r 'vscode-ssh/' 'extension' && sudo zip -r 'vscode-ssh.vsix' 'extension' && mv 'vscode-ssh.vsix' 'vscode-ssh/vscode-ssh.vsix' && sudo rm -rf 'extension' && cd vscode-ssh"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@types/vscode": "1.38.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.3.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "xterm": "^4.6.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-search": "^0.5.0",
    "xterm-addon-search-bar": "^0.2.0",
    "xterm-addon-web-links": "^0.2.1"
  },
  "dependencies": {
    "@vscode/vsce": "^3.2.1",
    "debug": "2.6.9",
    "element-ui": "^2.13.2",
    "lodash.defaults": "^4.1.0",
    "pretty-bytes": "^5.3.0",
    "progress-stream": "^2.0.0",
    "ssh2": "^1.15.0",
    "tunnel-ssh": "^4.1.4",
    "vsce": "^2.15.0",
    "vue": "^2.6.11"
  }
}
